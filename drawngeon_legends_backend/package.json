{
  "name": "drawngeon_backend",
  "version": "1.0.0",
  "description": "Back-end for Drawngeon-Legends, an adventure for RPG fans and masters alike.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ttsc -p tsconfig.build.json",
    "prepare": "husky install",
    "start": "node dist/server.js",
    "migration:run": "prisma migrate deploy",
    "migration:generate": "npm run dev:load-envs prisma migrate dev",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "test": "NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:coverage": "NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "NODE_ENV=development nodemon --legacy-watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "dev:migrate": "npx prisma migrate dev && npm run dev:load-envs prisma db seed && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcandeloni/drawngeon_legends_backend.git"
  },
  "keywords": [
    "drawngeon-legends",
    "back-end",
    "typescript",
    "rpg",
    "postgres",
    "express",
    "nodejs"
  ],
  "author": "Kevin Candeloni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kcandeloni/drawngeon_legends_backend/issues"
  },
  "homepage": "https://github.com/kcandeloni/drawngeon_legends_backend#readme",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "axios": "^1.2.3",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-boundaries": "^3.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  }
}
